{
	"name": "solid-collapse",
	"version": "0.9.0",
	"private": "false",
	"description": "Tiny and performant collapse component for SolidJS.",
	"keywords": [
		"solid",
		"collapse",
		"accordion",
		"solid-js",
		"solid-collapse",
		"solid-accordion"
	],
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js"
		}
	},
	"module": "dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rimraf dist && vite build",
		"postbuild": "cpy dist/Collapse.d.ts dist --rename index.d.ts --flat && rimraf dist/Collapse.d.ts",
		"build:examples": "cd examples && yarn && yarn build",
		"dev": "vite",
		"serve": "vite preview",
		"start": "vite"
	},
	"lint-staged": {
		"*.{ts,tsx}": "eslint --cache --fix",
		"*.{ts,tsx,css,md}": "prettier --write"
	},
	"dependencies": {},
	"devDependencies": {
		"cpy-cli": "^4.1.0",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"solid-js": "^1.4.7",
		"typescript": "^4.7.4",
		"vite": "^3.0.0",
		"vite-plugin-dts": "^1.4.1",
		"vite-plugin-solid": "^2.3.0"
	}
}
